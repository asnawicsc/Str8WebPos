         <div class="content-wrapper">
            <div class="row">
               <div class="col-xl-12">
            <%= render @view_module, @view_template, assigns %>
         </div></div>
         </div>
      </section>
      <!-- Page footer-->
      <footer class="footer-container">
         <span>&copy; 2019 - <span>STR<span style="font-size: 15px; font-weight: bolder;">8</span>POS</span></span>
      </footer>
